### 2016 Fort McMurray Wildfire

kb:ThreatEvent_FortMcMurray2016 rdf:type ao:ThreatEvent ;
    rdfs:label "2016 Fort McMurray Wildfire Threat" ;
    ao:occurrenceDate "2016-05-03"^^xsd:date ;
    ao:causedBy kb:Peril_Wildfire ;
    rdfs:comment "Horse River wildfire that forced evacuation of Fort McMurray" .

kb:LossEvent_FortMcMurray2016 rdf:type ao:LossEvent ;
    rdfs:label "2016 Fort McMurray Wildfire Losses" ;
    ao:occurrenceDate "2016-05-03"^^xsd:date ;
    rdfs:comment "Costliest natural disaster in Canadian history - destroyed 2,400 structures" .

kb:ThreatEvent_FortMcMurray2016 ao:precipitates kb:LossEvent_FortMcMurray2016 .

kb:Loss_FortMcMurray2016 rdf:type ao:Loss , ao:UltimateClaimCost ;
    rdfs:label "Fort McMurray Industry Insured Loss" ;
    ao:hasSeverity "3580000000.00"^^xsd:decimal ;
    rdfs:comment "Industry insured loss of $3.58 billion CAD" .

kb:LossEvent_FortMcMurray2016 ao:resultsIn kb:Loss_FortMcMurray2016 .

