#################################################################
#    Canadian P&C Insurance Knowledge Base (Improved)
#################################################################
#
#    This knowledge base demonstrates property and casualty insurance
#    concepts in the Canadian market, properly aligned with the
#    Actuarial Ontology.
#
#    Key improvements over original:
#    - Fixed ontology property usage (insures, hasPolicy, causedBy)
#    - Classified perils as Natural vs Man-made
#    - Added insurance product types
#    - Added premium information
#    - Added claim reserves
#    - Added coverage details (deductibles, limits)
#    - Added actuaries and actuarial activities
#    - Added catastrophe models
#    - Added regulators
#    - Added reinsurance relationships
#    - Added data quality metadata
#    - Properly modeled threat/loss event chains
#
#################################################################

#################################################################
#    Perils (Properly Classified)
#################################################################

### Natural Perils

kb:Peril_Fire rdf:type ao:NaturalPeril ;
    rdfs:label "Fire" ;
    rdfs:comment "Uncontrolled combustion causing property damage" .

kb:Peril_Flood rdf:type ao:NaturalPeril ;
    rdfs:label "Flood" ;
    rdfs:comment "Overflow of water onto normally dry land" .

kb:Peril_Wildfire rdf:type ao:NaturalPeril ;
    rdfs:label "Wildfire" ;
    rdfs:comment "Uncontrolled fire in wilderness or rural areas" .

kb:Peril_IceStorm rdf:type ao:NaturalPeril ;
    rdfs:label "Ice Storm" ;
    rdfs:comment "Freezing rain accumulation causing damage" .

kb:Peril_Hurricane rdf:type ao:NaturalPeril ;
    rdfs:label "Hurricane" ;
    rdfs:comment "Tropical cyclone with sustained winds over 119 km/h" .

kb:Peril_Hail rdf:type ao:NaturalPeril ;
    rdfs:label "Hail" ;
    rdfs:comment "Precipitation in the form of ice pellets" .

kb:Peril_Windstorm rdf:type ao:NaturalPeril ;
    rdfs:label "Windstorm" ;
    rdfs:comment "Strong winds causing property damage" .

kb:Peril_Earthquake rdf:type ao:NaturalPeril ;
    rdfs:label "Earthquake" ;
    rdfs:comment "Seismic activity causing ground shaking" .

kb:Peril_Tornado rdf:type ao:NaturalPeril ;
    rdfs:label "Tornado" ;
    rdfs:comment "Violently rotating column of air" .

kb:Peril_Drought rdf:type ao:NaturalPeril ;
    rdfs:label "Drought" ;
    rdfs:comment "Extended period of abnormally low rainfall" .

kb:Peril_Cyclone rdf:type ao:NaturalPeril ;
    rdfs:label "Cyclone" ;
    rdfs:comment "Large scale air mass rotating around a low pressure center" .

kb:Peril_Lightning rdf:type ao:NaturalPeril ;
    rdfs:label "Lightning" ;
    rdfs:comment "Electrical discharge during thunderstorms" .

### Man-made Perils

kb:Peril_Explosion rdf:type ao:ManmadePeril ;
    rdfs:label "Explosion" ;
    rdfs:comment "Sudden release of energy causing blast damage" .

kb:Peril_Theft rdf:type ao:ManmadePeril ;
    rdfs:label "Theft" ;
    rdfs:comment "Unauthorized taking of property" .

kb:Peril_Riot rdf:type ao:ManmadePeril ;
    rdfs:label "Riot or Civil Commotion" ;
    rdfs:comment "Violent disturbance by a group" .

kb:Peril_Vandalism rdf:type ao:ManmadePeril ;
    rdfs:label "Vandalism" ;
    rdfs:comment "Deliberate destruction or damage to property" .

kb:Peril_Vehicle rdf:type ao:ManmadePeril ;
    rdfs:label "Vehicle Impact" ;
    rdfs:comment "Damage caused by collision with vehicle" .

kb:Peril_Water rdf:type ao:ManmadePeril ;
    rdfs:label "Water Damage" ;
    rdfs:comment "Damage from burst pipes, appliance leaks, etc." .

