ao:sellsThrough rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Insurer ;
    rdfs:range ao:DistributionChannel ;
    rdfs:label "sells through"@en ;
    rdfs:comment "Relates an insurer to the distribution channels it uses to market and sell products."@en .

ao:representsInsurer rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Agent ;
    rdfs:range ao:Insurer ;
    rdfs:label "represents insurer"@en ;
    rdfs:comment "Relates an agent to an insurer they represent in selling insurance."@en .

ao:investigates rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ClaimsAdjuster ;
    rdfs:range ao:Claim ;
    rdfs:label "investigates"@en ;
    rdfs:comment "Relates a claims adjuster to a claim they are investigating."@en .

ao:settles rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ClaimsAdjuster ;
    rdfs:range ao:Claim ;
    rdfs:label "settles"@en ;
    rdfs:comment "Relates a claims adjuster to a claim they settle."@en .

ao:cedesTo rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Insurer ;
    rdfs:range ao:Reinsurer ;
    rdfs:label "cedes to"@en ;
