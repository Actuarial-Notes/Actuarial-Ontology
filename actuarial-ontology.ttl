@prefix : <http://actuarialnotes.com/ontology/actuarial#> .
@prefix ao: <http://actuarialnotes.com/ontology/actuarial#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<http://actuarialnotes.com/ontology/actuarial> rdf:type owl:Ontology ;
    dcterms:title "Actuarial Ontology"@en ;
    dcterms:description "An ontology for the domain of actuarial science - the discipline of measuring and managing financial risk."@en ;
    dcterms:created "2026-01-10"^^xsd:date ;
    dcterms:modified "2026-01-11"^^xsd:date ;
    dcterms:license <https://opensource.org/licenses/MIT> ;
    owl:versionInfo "0.2.0-draft" ;
    rdfs:comment "This ontology defines core concepts, relationships, and practices in actuarial science across insurance, pensions, investment, and risk management domains. Aligned with UFO (Unified Foundational Ontology), COVER (Common Ontology of Value and Risk), and ASOPs (Actuarial Standards of Practice)."@en .

#################################################################
#    Annotation Properties
#################################################################

### UFO Meta-Properties

ao:rigidity rdf:type owl:AnnotationProperty ;
    rdfs:label "rigidity"@en ;
    rdfs:comment "Indicates whether instances can change type while maintaining identity. Values: rigid (cannot change), anti-rigid (can change), semi-rigid (some can change). Based on UFO ontological meta-properties."@en .

ao:ufoCategory rdf:type owl:AnnotationProperty ;
    rdfs:label "UFO category"@en ;
    rdfs:comment "Classifies types according to UFO ontological categories. Values: kind (rigid sortal providing identity), role (anti-rigid relational), phase (anti-rigid intrinsic), category, mixin."@en .

ao:asopReference rdf:type owl:AnnotationProperty ;
    rdfs:label "ASOP reference"@en ;
    rdfs:comment "References specific Actuarial Standard of Practice (ASOP) number and section."@en .

#################################################################
#    Foundational Classes (UFO Alignment)
#################################################################

### Top-Level Ontological Distinctions

ao:Endurant rdf:type owl:Class ;
    rdfs:label "Endurant"@en ;
    rdfs:comment "An entity that exists in time with all its parts present at once. Endurants persist through time maintaining their identity (UFO-A). Examples: persons, organizations, objects, policies."@en ;
    ao:ufoCategory "category" .

ao:Perdurant rdf:type owl:Class ;
    rdfs:label "Perdurant"@en ;
    rdfs:comment "An entity that unfolds in time, accumulating temporal parts. Perdurants occur or happen in time (UFO-B). Examples: events, processes, activities."@en ;
    ao:ufoCategory "category" ;
    owl:disjointWith ao:Endurant .

ao:Moment rdf:type owl:Class ;
    rdfs:subClassOf ao:Endurant ;
    rdfs:label "Moment"@en ;
    rdfs:comment "An intrinsic property or aspect that inheres in (depends on) another endurant. Moments cannot exist independently (UFO-A). Examples: qualities, measurements, dispositions."@en ;
    ao:ufoCategory "category" .

#################################################################
#    Object Properties
#################################################################

### Core Relationships

ao:hasAgent rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Activity ;
    rdfs:range ao:Agent ;
    rdfs:label "has agent"@en ;
    rdfs:comment "Relates an activity to an agent that performs or participates in it."@en .

ao:manages rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Agent ;
    rdfs:range ao:Risk ;
    rdfs:label "manages"@en ;
    rdfs:comment "Relates an agent to a risk that they manage."@en .

ao:exposedTo rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Agent ;
    rdfs:range ao:Risk ;
    rdfs:label "exposed to"@en ;
    rdfs:comment "Relates an agent to a risk that they are exposed to."@en .

ao:hasRisk rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Entity ;
    rdfs:range ao:Risk ;
    rdfs:label "has risk"@en ;
    rdfs:comment "Relates an entity to a risk associated with it."@en .

ao:causedBy rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Event ;
    rdfs:range ao:Peril ;
    rdfs:label "caused by"@en ;
    rdfs:comment "Relates an event to the peril that caused it."@en .

ao:resultsIn rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Event ;
    rdfs:range ao:Loss ;
    rdfs:label "results in"@en ;
    rdfs:comment "Relates an event to a loss that resulted from it."@en .

### Insurance Relationships

ao:insures rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Insurer ;
    rdfs:range ao:Insured ;
    rdfs:label "insures"@en ;
    rdfs:comment "Relates an insurer to an entity they insure."@en .

ao:hasPolicy rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Insured ;
    rdfs:range ao:InsurancePolicy ;
    rdfs:label "has policy"@en ;
    rdfs:comment "Relates an insured entity to their insurance policy."@en .

ao:covers rdf:type owl:ObjectProperty ;
    rdfs:domain ao:InsurancePolicy ;
    rdfs:range ao:Risk ;
    rdfs:label "covers"@en ;
    rdfs:comment "Relates an insurance policy to a risk it covers."@en .

ao:hasClaim rdf:type owl:ObjectProperty ;
    rdfs:domain ao:InsurancePolicy ;
    rdfs:range ao:Claim ;
    rdfs:label "has claim"@en ;
    rdfs:comment "Relates an insurance policy to a claim made against it."@en .

ao:triggeredBy rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Claim ;
    rdfs:range ao:Event ;
    rdfs:label "triggered by"@en ;
    rdfs:comment "Relates a claim to the event that triggered it."@en .

ao:hasReserve rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Claim ;
    rdfs:range ao:Reserve ;
    rdfs:label "has reserve"@en ;
    rdfs:comment "Relates a claim to the reserve established for it."@en .

### Financial Relationships

ao:hasAsset rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Entity ;
    rdfs:range ao:Asset ;
    rdfs:label "has asset"@en ;
    rdfs:comment "Relates an entity to an asset it holds."@en .

ao:hasLiability rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Entity ;
    rdfs:range ao:Liability ;
    rdfs:label "has liability"@en ;
    rdfs:comment "Relates an entity to a liability it owes."@en .

ao:investsIn rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Investor ;
    rdfs:range ao:Asset ;
    rdfs:label "invests in"@en ;
    rdfs:comment "Relates an investor to an asset they invest in."@en .

### Model Relationships

ao:usesModel rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ActuarialActivity ;
    rdfs:range ao:ActuarialModel ;
    rdfs:label "uses model"@en ;
    rdfs:comment "Relates an actuarial activity to a model it uses."@en .

ao:basedOnData rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ActuarialModel ;
    rdfs:range ao:Data ;
    rdfs:label "based on data"@en ;
    rdfs:comment "Relates an actuarial model to data it is based on."@en .

ao:producesEstimate rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ActuarialModel ;
    rdfs:range ao:Estimate ;
    rdfs:label "produces estimate"@en ;
    rdfs:comment "Relates an actuarial model to an estimate it produces."@en .

### UFO Participation Relationships

ao:participatesIn rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Endurant ;
    rdfs:range ao:Perdurant ;
    rdfs:label "participates in"@en ;
    rdfs:comment "Relates an endurant to a perdurant (event/activity) it participates in (UFO-B)."@en .

ao:hasParticipant rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Perdurant ;
    rdfs:range ao:Endurant ;
    rdfs:label "has participant"@en ;
    rdfs:comment "Relates a perdurant (event/activity) to an endurant that participates in it (UFO-B)."@en ;
    owl:inverseOf ao:participatesIn .

ao:inheresIn rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Moment ;
    rdfs:range ao:Endurant ;
    rdfs:label "inheres in"@en ;
    rdfs:comment "Relates a moment (property/quality) to the endurant it depends on (UFO-A)."@en .

### COVER Event Chain Relationships

ao:precipitates rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ThreatEvent ;
    rdfs:range ao:LossEvent ;
    rdfs:label "precipitates"@en ;
    rdfs:comment "Relates a threat event to a loss event it can cause (COVER)."@en .

ao:underminesObjectiveOf rdf:type owl:ObjectProperty ;
    rdfs:domain ao:LossEvent ;
    rdfs:range ao:RiskSubject ;
    rdfs:label "undermines objective of"@en ;
    rdfs:comment "Relates a loss event to the risk subject whose objectives are undermined (COVER)."@en .

ao:manifestsAs rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Disposition ;
    rdfs:range ao:Event ;
    rdfs:label "manifests as"@en ;
    rdfs:comment "Relates a disposition to an event it can manifest as (UFO/COVER)."@en .

### COVER Value Relationships

ao:hasValueFor rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ValueObject ;
    rdfs:range ao:Agent ;
    rdfs:label "has value for"@en ;
    rdfs:comment "Relates a value object to an agent for whom it has value (COVER)."@en .

ao:enablesValueFor rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ValueEnabler ;
    rdfs:range ao:Agent ;
    rdfs:label "enables value for"@en ;
    rdfs:comment "Relates a value enabler to an agent whose value experience it enables (COVER)."@en .

ao:ascribesValue rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Agent ;
    rdfs:range ao:ValueAscription ;
    rdfs:label "ascribes value"@en ;
    rdfs:comment "Relates an agent to a value ascription judgment they make (COVER)."@en .

ao:assessesRisk rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Agent ;
    rdfs:range ao:RiskAssessmentJudgment ;
    rdfs:label "assesses risk"@en ;
    rdfs:comment "Relates an agent to a risk assessment judgment they make (COVER)."@en .

### ASOP 41 Communication Relationships

ao:hasIntendedUser rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ActuarialCommunication ;
    rdfs:range ao:IntendedUser ;
    rdfs:label "has intended user"@en ;
    rdfs:comment "Relates an actuarial communication to its intended user (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

ao:hasDisclosure rdf:type owl:ObjectProperty ;
    rdfs:domain ao:ActuarialCommunication ;
    rdfs:range ao:Disclosure ;
    rdfs:label "has disclosure"@en ;
    rdfs:comment "Relates an actuarial communication to information disclosed within it (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

ao:communicates rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Actuary ;
    rdfs:range ao:ActuarialCommunication ;
    rdfs:label "communicates"@en ;
    rdfs:comment "Relates an actuary to a communication they produce (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

#################################################################
#    Data Properties
#################################################################

### Measurement Properties

ao:hasValue rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Measurement ;
    rdfs:range xsd:decimal ;
    rdfs:label "has value"@en ;
    rdfs:comment "The numeric value of a measurement."@en .

ao:hasMonetaryValue rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:FinancialMeasurement ;
    rdfs:range xsd:decimal ;
    rdfs:label "has monetary value"@en ;
    rdfs:comment "The monetary value in base currency."@en .

ao:hasProbability rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Event ;
    rdfs:range xsd:decimal ;
    rdfs:label "has probability"@en ;
    rdfs:comment "The probability of an event occurring (0 to 1)."@en .

ao:hasSeverity rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Loss ;
    rdfs:range xsd:decimal ;
    rdfs:label "has severity"@en ;
    rdfs:comment "The magnitude or severity of a loss."@en .

ao:hasFrequency rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Event ;
    rdfs:range xsd:decimal ;
    rdfs:label "has frequency"@en ;
    rdfs:comment "The frequency of occurrence of an event type."@en .

### Temporal Properties

ao:effectiveDate rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:InsurancePolicy ;
    rdfs:range xsd:date ;
    rdfs:label "effective date"@en ;
    rdfs:comment "The date when an insurance policy becomes effective."@en .

ao:expirationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:InsurancePolicy ;
    rdfs:range xsd:date ;
    rdfs:label "expiration date"@en ;
    rdfs:comment "The date when an insurance policy expires."@en .

ao:occurrenceDate rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Event ;
    rdfs:range xsd:dateTime ;
    rdfs:label "occurrence date"@en ;
    rdfs:comment "The date and time when an event occurred."@en .

ao:reportingDate rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Claim ;
    rdfs:range xsd:date ;
    rdfs:label "reporting date"@en ;
    rdfs:comment "The date when a claim was reported."@en .

### Identification Properties

ao:policyNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:InsurancePolicy ;
    rdfs:range xsd:string ;
    rdfs:label "policy number"@en ;
    rdfs:comment "The unique identifier for an insurance policy."@en .

ao:claimNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Claim ;
    rdfs:range xsd:string ;
    rdfs:label "claim number"@en ;
    rdfs:comment "The unique identifier for a claim."@en .

#################################################################
#    Classes
#################################################################

### Top-Level Categories (UFO-Aligned)

ao:Entity rdf:type owl:Class ;
    rdfs:subClassOf ao:Endurant ;
    rdfs:label "Entity"@en ;
    rdfs:comment "A substantial endurant - a thing that exists and can be distinguished from other things. Entities persist through time maintaining their identity."@en ;
    ao:ufoCategory "category" .

ao:Agent rdf:type owl:Class ;
    rdfs:subClassOf ao:Entity ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An entity with intentionality that can act, make decisions, or participate in activities (UFO-C). Agents have beliefs, desires, and intentions."@en ;
    ao:ufoCategory "category" .

ao:Object rdf:type owl:Class ;
    rdfs:subClassOf ao:Entity ;
    rdfs:label "Object"@en ;
    rdfs:comment "A substantial endurant that does not have agency or perform intentional actions. Objects persist through time but lack intentionality."@en ;
    ao:ufoCategory "category" ;
    owl:disjointWith ao:Agent .

ao:Event rdf:type owl:Class ;
    rdfs:subClassOf ao:Perdurant ;
    rdfs:label "Event"@en ;
    rdfs:comment "An atomic perdurant - something that happens at a particular time and place. Events are temporal occurrences without complex internal structure (UFO-B)."@en ;
    ao:ufoCategory "category" .

ao:Activity rdf:type owl:Class ;
    rdfs:subClassOf ao:Perdurant ;
    rdfs:label "Activity"@en ;
    rdfs:comment "A complex perdurant - a process or action performed by an agent over time. Activities have temporal parts and internal structure (UFO-B)."@en ;
    ao:ufoCategory "category" .

### Agent Types (Kinds and Roles)

ao:Person rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Person"@en ;
    rdfs:comment "A human individual. This is a kind - a person cannot cease to be a person while existing."@en ;
    ao:rigidity "rigid" ;
    ao:ufoCategory "kind" .

ao:Organization rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Organization"@en ;
    rdfs:comment "A structured group of people with a collective goal. This is a kind - an organization maintains this identity throughout its existence."@en ;
    ao:rigidity "rigid" ;
    ao:ufoCategory "kind" .

ao:Actuary rdf:type owl:Class ;
    rdfs:subClassOf ao:Person ;
    rdfs:label "Actuary"@en ;
    rdfs:comment "A role played by a person who is a professional in actuarial science, measuring and managing financial risk. A person can become or cease to be an actuary."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Insurer rdf:type owl:Class ;
    rdfs:subClassOf ao:Organization ;
    rdfs:label "Insurer"@en ;
    rdfs:comment "A role played by an organization that provides insurance coverage. An organization can start or stop being an insurer."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Insured rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Insured"@en ;
    rdfs:comment "A role played by an agent that has insurance coverage. An agent can become insured or lose insurance status."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Policyholder rdf:type owl:Class ;
    rdfs:subClassOf ao:Insured ;
    rdfs:label "Policyholder"@en ;
    rdfs:comment "A role played by an agent that holds an insurance policy. This is a subrole of Insured."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Beneficiary rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Beneficiary"@en ;
    rdfs:comment "A role played by an agent that receives benefits from an insurance policy or pension."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Reinsurer rdf:type owl:Class ;
    rdfs:subClassOf ao:Insurer ;
    rdfs:label "Reinsurer"@en ;
    rdfs:comment "A specialized role played by an insurer that provides insurance to other insurers."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Investor rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Investor"@en ;
    rdfs:comment "A role played by an agent that allocates capital with the expectation of future returns."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Regulator rdf:type owl:Class ;
    rdfs:subClassOf ao:Organization ;
    rdfs:label "Regulator"@en ;
    rdfs:comment "A role played by an organization that oversees and enforces regulations in the insurance or financial industry."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

### Risk Concepts (COVER-Aligned)

ao:Risk rdf:type owl:Class ;
    rdfs:label "Risk"@en ;
    rdfs:comment "Abstract concept representing potential for loss or deviation from objectives. COVER distinguishes three facets: quantitative risk (numeric measure), risk experience (event chain), and risk assessment (agent judgment)."@en .

ao:RiskSubject rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Risk Subject"@en ;
    rdfs:comment "A role played by an agent whose objectives may be undermined by loss events. The entity exposed to and bearing risk (COVER)."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

### COVER's Three Risk Distinctions

ao:QuantitativeRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Measurement ;
    rdfs:label "Quantitative Risk"@en ;
    rdfs:comment "A numerical measure or magnitude of risk, typically expressed as probability Ã— severity or similar metrics (COVER). This is risk as a number."@en .

ao:RiskExperience rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Risk Experience"@en ;
    rdfs:comment "A multifaceted hypothetical or actual occurrence involving threat and loss events affecting a risk subject. This is risk as a chain of events (COVER)."@en .

ao:RiskAssessmentJudgment rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Risk Assessment Judgment"@en ;
    rdfs:comment "A perceptual ascription of risk by an agent to an object or experience. This is risk as agent judgment, a special case of value ascription (COVER)."@en .

### Traditional Risk Classifications

ao:InsurableRisk rdf:type owl:Class ;
    rdfs:label "Insurable Risk"@en ;
    rdfs:comment "A risk classification for risks that meet criteria for insurance coverage (random, fortuitous, measurable, definable, etc.). Can apply to any of the three COVER facets."@en .

ao:UninsurableRisk rdf:type owl:Class ;
    rdfs:label "Uninsurable Risk"@en ;
    rdfs:comment "A risk classification for risks that do not meet criteria for insurance coverage. Can apply to any of the three COVER facets."@en .

ao:SystemicRisk rdf:type owl:Class ;
    rdfs:label "Systemic Risk"@en ;
    rdfs:comment "A risk that affects an entire market or system, not just individual participants."@en .

ao:IdiosyncraticRisk rdf:type owl:Class ;
    rdfs:label "Idiosyncratic Risk"@en ;
    rdfs:comment "A risk that is specific to a particular entity or asset, not affecting the broader system."@en .

### Risk Categories

ao:MortalityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Mortality Risk"@en ;
    rdfs:comment "A risk associated with death occurring sooner than expected."@en .

ao:LongevityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Longevity Risk"@en ;
    rdfs:comment "A risk associated with people living longer than expected."@en .

ao:MorbidityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Morbidity Risk"@en ;
    rdfs:comment "A risk associated with illness or disability."@en .

ao:PropertyRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Property Risk"@en ;
    rdfs:comment "A risk of loss or damage to physical property."@en .

ao:LiabilityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Liability Risk"@en ;
    rdfs:comment "A risk of being held legally responsible for harm or damages."@en .

ao:MarketRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Market Risk"@en ;
    rdfs:comment "A risk of losses due to changes in market prices or rates."@en .

ao:CreditRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Credit Risk"@en ;
    rdfs:comment "A risk that a borrower will default on their obligations."@en .

ao:OperationalRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Operational Risk"@en ;
    rdfs:comment "A risk of loss from inadequate or failed processes, people, or systems."@en .

ao:CatastropheRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Catastrophe Risk"@en ;
    rdfs:comment "A risk of loss from extreme events affecting many insureds simultaneously."@en .

### Perils and Events (COVER Event Chain)

ao:Peril rdf:type owl:Class ;
    rdfs:label "Peril"@en ;
    rdfs:comment "A specific cause of loss or damage (e.g., fire, flood, theft). Perils are sources that can trigger threat events."@en .

ao:NaturalPeril rdf:type owl:Class ;
    rdfs:subClassOf ao:Peril ;
    rdfs:label "Natural Peril"@en ;
    rdfs:comment "A peril caused by natural forces (e.g., earthquake, hurricane, flood)."@en .

ao:ManmadePeril rdf:type owl:Class ;
    rdfs:subClassOf ao:Peril ;
    rdfs:label "Man-made Peril"@en ;
    rdfs:comment "A peril caused by human actions (e.g., fire, theft, terrorism)."@en .

ao:ThreatEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Threat Event"@en ;
    rdfs:comment "A hypothetical or actual event capable of precipitating a loss event. Threat events form the link between perils and losses in COVER's event chain (COVER)."@en .

ao:LossEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Loss Event"@en ;
    rdfs:comment "An event that results in financial loss and undermines the objectives of a risk subject. Loss events are precipitated by threat events (COVER)."@en .

ao:ClaimEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Claim Event"@en ;
    rdfs:comment "An event that triggers an insurance claim. A specific type of loss event that activates insurance coverage."@en .

### COVER Value Concepts

ao:Value rdf:type owl:Class ;
    rdfs:label "Value"@en ;
    rdfs:comment "The worth, importance, or usefulness of something to an agent. COVER shows that risk is fundamentally intertwined with value (COVER)."@en .

ao:ValueObject rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Value Object"@en ;
    rdfs:comment "An object whose dispositions enable value experiences for an agent. Objects that have value to a value subject (COVER)."@en .

ao:ValueEnabler rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Value Enabler"@en ;
    rdfs:comment "An object whose dispositions enable the occurrence of value experiences (COVER)."@en .

ao:ValueExperience rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Value Experience"@en ;
    rdfs:comment "An event that realizes or delivers value for a value subject. Parallel concept to risk experience (COVER)."@en .

ao:ValueAscription rdf:type owl:Class ;
