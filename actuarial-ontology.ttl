    rdfs:range xsd:decimal ;
    rdfs:label "accuracy"@en ;
    rdfs:comment "A measure of how closely data values match the true or reference values. Higher accuracy indicates less error (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

ao:relevance rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:DataQuality ;
    rdfs:range xsd:decimal ;
    rdfs:label "relevance"@en ;
    rdfs:comment "A measure of the degree to which data is applicable and helpful for the intended purpose (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

ao:timeliness rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:DataQuality ;
    rdfs:range xsd:decimal ;
    rdfs:label "timeliness"@en ;
    rdfs:comment "A measure of how current and up-to-date the data is relative to the time period being analyzed (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

### ASOP 25: Credibility Properties

ao:credibilityWeight rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Credibility ;
    rdfs:range xsd:decimal ;
    rdfs:label "credibility weight"@en ;
    rdfs:comment "A numeric weight (typically 0 to 1) representing the degree of credibility assigned to data. A weight of 1 indicates full credibility, while values less than 1 indicate partial credibility (ASOP 25)."@en ;
    ao:asopReference "ASOP 25" .

ao:credibilityStandard rdf:type owl:DatatypeProperty ;
    rdfs:domain ao:Credibility ;
    rdfs:range xsd:string ;
    rdfs:label "credibility standard"@en ;
    rdfs:comment "The standard or criterion used to determine credibility, such as 'limited fluctuation credibility' or 'greatest accuracy credibility' (ASOP 25)."@en ;
    ao:asopReference "ASOP 25" .

#################################################################
#    Classes
#################################################################

### Top-Level Categories (UFO-Aligned)

ao:Entity rdf:type owl:Class ;
    rdfs:subClassOf ao:Endurant ;
    rdfs:label "Entity"@en ;
    rdfs:comment "A substantial endurant - a thing that exists and can be distinguished from other things. Entities persist through time maintaining their identity."@en ;
    ao:ufoCategory "category" .

ao:Agent rdf:type owl:Class ;
    rdfs:subClassOf ao:Entity ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An entity with intentionality that can act, make decisions, or participate in activities (UFO-C). Agents have beliefs, desires, and intentions."@en ;
    ao:ufoCategory "category" .

ao:Object rdf:type owl:Class ;
    rdfs:subClassOf ao:Entity ;
    rdfs:label "Object"@en ;
    rdfs:comment "A substantial endurant that does not have agency or perform intentional actions. Objects persist through time but lack intentionality."@en ;
    ao:ufoCategory "category" ;
    owl:disjointWith ao:Agent .

ao:Event rdf:type owl:Class ;
    rdfs:subClassOf ao:Perdurant ;
    rdfs:label "Event"@en ;
    rdfs:comment "An atomic perdurant - something that happens at a particular time and place. Events are temporal occurrences without complex internal structure (UFO-B)."@en ;
    ao:ufoCategory "category" .

ao:Activity rdf:type owl:Class ;
    rdfs:subClassOf ao:Perdurant ;
    rdfs:label "Activity"@en ;
    rdfs:comment "A complex perdurant - a process or action performed by an agent over time. Activities have temporal parts and internal structure (UFO-B)."@en ;
    ao:ufoCategory "category" .

### Agent Types (Kinds and Roles)

ao:Person rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Person"@en ;
    rdfs:comment "A human individual. This is a kind - a person cannot cease to be a person while existing."@en ;
    ao:rigidity "rigid" ;
    ao:ufoCategory "kind" .

ao:Organization rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Organization"@en ;
    rdfs:comment "A structured group of people with a collective goal. This is a kind - an organization maintains this identity throughout its existence."@en ;
    ao:rigidity "rigid" ;
    ao:ufoCategory "kind" .

ao:Actuary rdf:type owl:Class ;
    rdfs:subClassOf ao:Person ;
    rdfs:label "Actuary"@en ;
    rdfs:comment "A role played by a person who is a professional in actuarial science, measuring and managing financial risk. A person can become or cease to be an actuary."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Insurer rdf:type owl:Class ;
    rdfs:subClassOf ao:Organization ;
    rdfs:label "Insurer"@en ;
    rdfs:comment "A role played by an organization that provides insurance coverage. An organization can start or stop being an insurer."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Insured rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Insured"@en ;
    rdfs:comment "A role played by an agent that has insurance coverage. An agent can become insured or lose insurance status."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Policyholder rdf:type owl:Class ;
    rdfs:subClassOf ao:Insured ;
    rdfs:label "Policyholder"@en ;
    rdfs:comment "A role played by an agent that holds an insurance policy. This is a subrole of Insured."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Beneficiary rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Beneficiary"@en ;
    rdfs:comment "A role played by an agent that receives benefits from an insurance policy or pension."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Reinsurer rdf:type owl:Class ;
    rdfs:subClassOf ao:Insurer ;
    rdfs:label "Reinsurer"@en ;
    rdfs:comment "A specialized role played by an insurer that provides insurance to other insurers."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Investor rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Investor"@en ;
    rdfs:comment "A role played by an agent that allocates capital with the expectation of future returns."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

ao:Regulator rdf:type owl:Class ;
    rdfs:subClassOf ao:Organization ;
    rdfs:label "Regulator"@en ;
    rdfs:comment "A role played by an organization that oversees and enforces regulations in the insurance or financial industry."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

### Risk Concepts (COVER-Aligned)

ao:Risk rdf:type owl:Class ;
    rdfs:label "Risk"@en ;
    rdfs:comment "Abstract concept representing potential for loss or deviation from objectives. COVER distinguishes three facets: quantitative risk (numeric measure), risk experience (event chain), and risk assessment (agent judgment)."@en .

ao:RiskSubject rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Risk Subject"@en ;
    rdfs:comment "A role played by an agent whose objectives may be undermined by loss events. The entity exposed to and bearing risk (COVER)."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

### COVER's Three Risk Distinctions

ao:QuantitativeRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Measurement ;
    rdfs:label "Quantitative Risk"@en ;
    rdfs:comment "A numerical measure or magnitude of risk, typically expressed as probability Ã— severity or similar metrics (COVER). This is risk as a number."@en .

ao:RiskExperience rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Risk Experience"@en ;
    rdfs:comment "A multifaceted hypothetical or actual occurrence involving threat and loss events affecting a risk subject. This is risk as a chain of events (COVER)."@en .

ao:RiskAssessmentJudgment rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Risk Assessment Judgment"@en ;
    rdfs:comment "A perceptual ascription of risk by an agent to an object or experience. This is risk as agent judgment, a special case of value ascription (COVER)."@en .

### Traditional Risk Classifications

ao:InsurableRisk rdf:type owl:Class ;
    rdfs:label "Insurable Risk"@en ;
    rdfs:comment "A risk classification for risks that meet criteria for insurance coverage (random, fortuitous, measurable, definable, etc.). Can apply to any of the three COVER facets."@en .

ao:UninsurableRisk rdf:type owl:Class ;
    rdfs:label "Uninsurable Risk"@en ;
    rdfs:comment "A risk classification for risks that do not meet criteria for insurance coverage. Can apply to any of the three COVER facets."@en .

ao:SystemicRisk rdf:type owl:Class ;
    rdfs:label "Systemic Risk"@en ;
    rdfs:comment "A risk that affects an entire market or system, not just individual participants."@en .

ao:IdiosyncraticRisk rdf:type owl:Class ;
    rdfs:label "Idiosyncratic Risk"@en ;
    rdfs:comment "A risk that is specific to a particular entity or asset, not affecting the broader system."@en .

### Risk Categories

ao:MortalityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Mortality Risk"@en ;
    rdfs:comment "A risk associated with death occurring sooner than expected."@en .

ao:LongevityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Longevity Risk"@en ;
    rdfs:comment "A risk associated with people living longer than expected."@en .

ao:MorbidityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Morbidity Risk"@en ;
    rdfs:comment "A risk associated with illness or disability."@en .

ao:PropertyRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Property Risk"@en ;
    rdfs:comment "A risk of loss or damage to physical property."@en .

ao:LiabilityRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Liability Risk"@en ;
    rdfs:comment "A risk of being held legally responsible for harm or damages."@en .

ao:MarketRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Market Risk"@en ;
    rdfs:comment "A risk of losses due to changes in market prices or rates."@en .

ao:CreditRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Credit Risk"@en ;
    rdfs:comment "A risk that a borrower will default on their obligations."@en .

ao:OperationalRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Operational Risk"@en ;
    rdfs:comment "A risk of loss from inadequate or failed processes, people, or systems."@en .

ao:ModelRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Model Risk"@en ;
    rdfs:comment "The risk of adverse consequences from decisions based on incorrect or misused model outputs and reports. Model risk can arise from errors in model design, implementation, or application (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" .

ao:CatastropheRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurableRisk ;
    rdfs:label "Catastrophe Risk"@en ;
    rdfs:comment "A risk of loss from extreme events affecting many insureds simultaneously."@en .

### Perils and Events (COVER Event Chain)

ao:Peril rdf:type owl:Class ;
    rdfs:label "Peril"@en ;
    rdfs:comment "A specific cause of loss or damage (e.g., fire, flood, theft). Perils are sources that can trigger threat events."@en .

ao:NaturalPeril rdf:type owl:Class ;
    rdfs:subClassOf ao:Peril ;
    rdfs:label "Natural Peril"@en ;
    rdfs:comment "A peril caused by natural forces (e.g., earthquake, hurricane, flood)."@en .

ao:ManmadePeril rdf:type owl:Class ;
    rdfs:subClassOf ao:Peril ;
    rdfs:label "Man-made Peril"@en ;
    rdfs:comment "A peril caused by human actions (e.g., fire, theft, terrorism)."@en .

ao:ThreatEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Threat Event"@en ;
    rdfs:comment "A hypothetical or actual event capable of precipitating a loss event. Threat events form the link between perils and losses in COVER's event chain (COVER)."@en .

ao:LossEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Loss Event"@en ;
    rdfs:comment "An event that results in financial loss and undermines the objectives of a risk subject. Loss events are precipitated by threat events (COVER)."@en .

ao:ClaimEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Claim Event"@en ;
    rdfs:comment "An event that triggers an insurance claim. A specific type of loss event that activates insurance coverage."@en .

### COVER Value Concepts

ao:Value rdf:type owl:Class ;
    rdfs:label "Value"@en ;
    rdfs:comment "The worth, importance, or usefulness of something to an agent. COVER shows that risk is fundamentally intertwined with value (COVER)."@en .

ao:ValueObject rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Value Object"@en ;
    rdfs:comment "An object whose dispositions enable value experiences for an agent. Objects that have value to a value subject (COVER)."@en .

ao:ValueEnabler rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Value Enabler"@en ;
    rdfs:comment "An object whose dispositions enable the occurrence of value experiences (COVER)."@en .

ao:ValueExperience rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Value Experience"@en ;
    rdfs:comment "An event that realizes or delivers value for a value subject. Parallel concept to risk experience (COVER)."@en .

ao:ValueAscription rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Value Ascription"@en ;
    rdfs:comment "A judgment by an agent about the value of an object or experience. Risk assessment is a special case of value ascription (COVER)."@en .

ao:ObjectAtRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Object at Risk"@en ;
    rdfs:comment "An object that can be affected by threat or loss events. Objects whose value may be undermined (COVER)."@en .

ao:RiskEnabler rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Risk Enabler"@en ;
    rdfs:comment "An object whose dispositions can enable the occurrence of threat or loss events (COVER)."@en .

### Dispositions (UFO/COVER)

ao:Disposition rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Disposition"@en ;
    rdfs:comment "A tendency or capacity of an endurant to produce certain effects under specific conditions. Dispositions are moments that can manifest as events (UFO/COVER)."@en .

ao:Vulnerability rdf:type owl:Class ;
    rdfs:subClassOf ao:Disposition ;
    rdfs:label "Vulnerability"@en ;
    rdfs:comment "A disposition of an object at risk or risk enabler that can manifest as threat or loss events. A specific type of disposition related to risk (COVER)."@en .

### Insurance Products

ao:InsuranceProduct rdf:type owl:Class ;
    rdfs:label "Insurance Product"@en ;
    rdfs:comment "A type of insurance offering that provides coverage for specific risks."@en .

ao:LifeInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:InsuranceProduct ;
    rdfs:label "Life Insurance"@en ;
    rdfs:comment "An insurance product that pays benefits upon the death of the insured."@en .

ao:TermLifeInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:LifeInsurance ;
    rdfs:label "Term Life Insurance"@en ;
    rdfs:comment "A life insurance product that provides coverage for a specific term."@en .

ao:WholeLifeInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:LifeInsurance ;
    rdfs:label "Whole Life Insurance"@en ;
    rdfs:comment "A life insurance product that provides lifetime coverage with a savings component."@en .

ao:HealthInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:InsuranceProduct ;
    rdfs:label "Health Insurance"@en ;
    rdfs:comment "An insurance product that covers medical expenses."@en .

ao:PropertyInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:InsuranceProduct ;
    rdfs:label "Property Insurance"@en ;
    rdfs:comment "An insurance product that covers damage to property."@en .

ao:CasualtyInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:InsuranceProduct ;
    rdfs:label "Casualty Insurance"@en ;
    rdfs:comment "An insurance product that covers liability risks."@en .

ao:AutoInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:PropertyInsurance ;
    rdfs:label "Auto Insurance"@en ;
    rdfs:comment "An insurance product that covers vehicles."@en .

ao:HomeownersInsurance rdf:type owl:Class ;
    rdfs:subClassOf ao:PropertyInsurance ;
    rdfs:label "Homeowners Insurance"@en ;
    rdfs:comment "An insurance product that covers residential property and related liabilities."@en .

ao:Annuity rdf:type owl:Class ;
    rdfs:subClassOf ao:InsuranceProduct ;
    rdfs:label "Annuity"@en ;
    rdfs:comment "A financial product that provides a stream of payments over time."@en .

ao:Pension rdf:type owl:Class ;
    rdfs:label "Pension"@en ;
    rdfs:comment "A retirement plan that provides income to beneficiaries."@en .

ao:DefinedBenefitPension rdf:type owl:Class ;
    rdfs:subClassOf ao:Pension ;
    rdfs:label "Defined Benefit Pension"@en ;
    rdfs:comment "A pension that guarantees a specific benefit amount based on salary and service."@en .

ao:DefinedContributionPension rdf:type owl:Class ;
    rdfs:subClassOf ao:Pension ;
    rdfs:label "Defined Contribution Pension"@en ;
    rdfs:comment "A pension where the benefit depends on contributions and investment returns."@en .

### Insurance Contracts and Coverage

ao:InsurancePolicy rdf:type owl:Class ;
    rdfs:label "Insurance Policy"@en ;
    rdfs:comment "A contract between an insurer and insured that specifies coverage terms."@en .

ao:ReinsuranceContract rdf:type owl:Class ;
    rdfs:subClassOf ao:InsurancePolicy ;
    rdfs:label "Reinsurance Contract"@en ;
    rdfs:comment "A contract where one insurer transfers risk to another insurer."@en .

ao:Coverage rdf:type owl:Class ;
    rdfs:label "Coverage"@en ;
    rdfs:comment "The scope of protection provided by an insurance policy."@en .

ao:Exclusion rdf:type owl:Class ;
    rdfs:label "Exclusion"@en ;
    rdfs:comment "A provision that eliminates coverage for specific risks or circumstances."@en .

ao:Deductible rdf:type owl:Class ;
    rdfs:label "Deductible"@en ;
    rdfs:comment "An amount that the insured must pay before insurance coverage applies."@en .

ao:Limit rdf:type owl:Class ;
    rdfs:label "Limit"@en ;
    rdfs:comment "The maximum amount an insurer will pay under a policy."@en .

### Claims

ao:Claim rdf:type owl:Class ;
    rdfs:label "Claim"@en ;
    rdfs:comment "A formal request by an insured for payment under an insurance policy."@en .

ao:ReportedClaim rdf:type owl:Class ;
    rdfs:subClassOf ao:Claim ;
    rdfs:label "Reported Claim"@en ;
    rdfs:comment "A claim that has been reported to the insurer."@en .

ao:IncurredButNotReported rdf:type owl:Class ;
    rdfs:subClassOf ao:Claim ;
    rdfs:label "Incurred But Not Reported (IBNR)"@en ;
    rdfs:comment "A claim for a loss event that has occurred but has not yet been reported."@en .

ao:PaidClaim rdf:type owl:Class ;
    rdfs:subClassOf ao:ReportedClaim ;
    rdfs:label "Paid Claim"@en ;
    rdfs:comment "A phase - a claim that has been settled and paid. A claim can transition from outstanding to paid (UFO phase)."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "phase" .

ao:OutstandingClaim rdf:type owl:Class ;
    rdfs:subClassOf ao:ReportedClaim ;
    rdfs:label "Outstanding Claim"@en ;
    rdfs:comment "A phase - a claim that has been reported but not yet fully settled. A claim can transition to paid status (UFO phase)."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "phase" .

### Financial Concepts

ao:FinancialInstrument rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Financial Instrument"@en ;
    rdfs:comment "A tradable asset or contract that has monetary value."@en .

ao:Asset rdf:type owl:Class ;
    rdfs:subClassOf ao:FinancialInstrument ;
    rdfs:label "Asset"@en ;
    rdfs:comment "A resource with economic value owned by an entity."@en .

ao:Liability rdf:type owl:Class ;
    rdfs:subClassOf ao:FinancialInstrument ;
    rdfs:label "Liability"@en ;
    rdfs:comment "A financial obligation owed by an entity."@en .

ao:Reserve rdf:type owl:Class ;
    rdfs:subClassOf ao:Liability ;
    rdfs:label "Reserve"@en ;
    rdfs:comment "Funds set aside to meet future obligations."@en .

ao:ClaimReserve rdf:type owl:Class ;
    rdfs:subClassOf ao:Reserve ;
    rdfs:label "Claim Reserve"@en ;
    rdfs:comment "A reserve established for estimated claim payments."@en .

ao:CaseReserve rdf:type owl:Class ;
    rdfs:subClassOf ao:ClaimReserve ;
    rdfs:label "Case Reserve"@en ;
    rdfs:comment "A reserve for a specific reported claim."@en .

ao:IBNRReserve rdf:type owl:Class ;
    rdfs:subClassOf ao:ClaimReserve ;
    rdfs:label "IBNR Reserve"@en ;
    rdfs:comment "A reserve for incurred but not reported claims."@en .

ao:Premium rdf:type owl:Class ;
    rdfs:label "Premium"@en ;
    rdfs:comment "The amount paid by an insured for insurance coverage."@en .

ao:Loss rdf:type owl:Class ;
    rdfs:label "Loss"@en ;
    rdfs:comment "A reduction in value or financial detriment."@en .

ao:UltimateClaimCost rdf:type owl:Class ;
    rdfs:subClassOf ao:Loss ;
    rdfs:label "Ultimate Claim Cost"@en ;
    rdfs:comment "The total cost of a claim including all payments and expenses."@en .

ao:Capital rdf:type owl:Class ;
    rdfs:subClassOf ao:Asset ;
    rdfs:label "Capital"@en ;
    rdfs:comment "Financial resources available to support operations and absorb losses."@en .

ao:EconomicCapital rdf:type owl:Class ;
    rdfs:subClassOf ao:Capital ;
    rdfs:label "Economic Capital"@en ;
    rdfs:comment "The amount of capital needed to remain solvent at a given risk tolerance."@en .

ao:RegulatoryCapital rdf:type owl:Class ;
    rdfs:subClassOf ao:Capital ;
    rdfs:label "Regulatory Capital"@en ;
    rdfs:comment "The minimum capital required by regulators."@en .

### Actuarial Activities

ao:ActuarialActivity rdf:type owl:Class ;
    rdfs:subClassOf ao:Activity ;
    rdfs:label "Actuarial Activity"@en ;
    rdfs:comment "An activity performed by actuaries in measuring and managing financial risk."@en .

ao:RiskAssessment rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Risk Assessment"@en ;
    rdfs:comment "The process of identifying and evaluating risks. Can be decomposed into risk identification, measurement, and mitigation activities (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

### ASOP 51: Risk Assessment Components

ao:RiskIdentification rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Risk Identification"@en ;
    rdfs:comment "The process of systematically identifying risks that could affect an entity's objectives. Involves recognizing and documenting potential sources of uncertainty (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

ao:RiskMeasurement rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Risk Measurement"@en ;
    rdfs:comment "The process of quantifying the magnitude and likelihood of identified risks. Involves estimating potential impacts and probabilities (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

ao:RiskMitigation rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Risk Mitigation"@en ;
    rdfs:comment "The process of developing and implementing strategies to reduce, transfer, or control risks. Includes risk avoidance, reduction, sharing, and retention (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

ao:RiskMonitoring rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Risk Monitoring"@en ;
    rdfs:comment "The ongoing process of tracking identified risks, monitoring residual risks, identifying new risks, and evaluating risk mitigation effectiveness (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

ao:EnterpriseRiskManagement rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Enterprise Risk Management"@en ;
    rdfs:comment "A comprehensive, integrated approach to managing all risks across an organization. ERM considers interdependencies among risks and aligns risk management with strategic objectives (ASOP 58)."@en ;
    ao:asopReference "ASOP 58" .

ao:RiskTolerance rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Risk Tolerance"@en ;
    rdfs:comment "The acceptable level of variation in outcomes relative to objectives. Represents the amount of risk an entity is willing to accept in pursuit of its goals (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

ao:RiskAppetite rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Risk Appetite"@en ;
    rdfs:comment "The amount and type of risk an entity is willing to pursue or retain in order to achieve its objectives. Broader than risk tolerance (ASOP 51)."@en ;
    ao:asopReference "ASOP 51" .

ao:Pricing rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Pricing"@en ;
    rdfs:comment "The process of determining appropriate premium rates for insurance products."@en .

ao:Reserving rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Reserving"@en ;
    rdfs:comment "The process of estimating and setting aside funds for future obligations."@en .

ao:Valuation rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Valuation"@en ;
    rdfs:comment "The process of determining the economic value of assets, liabilities, or entities."@en .

ao:CapitalModeling rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Capital Modeling"@en ;
    rdfs:comment "The process of modeling capital requirements and risk exposures."@en .

ao:ExperienceAnalysis rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Experience Analysis"@en ;
    rdfs:comment "The process of analyzing historical data to understand past performance."@en .

ao:ProductDevelopment rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Product Development"@en ;
    rdfs:comment "The process of designing and developing new insurance products."@en .

### ASOP 56: Model Governance Activities

ao:ModelValidation rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Model Validation"@en ;
    rdfs:comment "The process of evaluating whether a model is appropriate for its intended purpose. Validation assesses model conceptual soundness, data quality, implementation correctness, and ongoing performance (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" .

ao:ModelCalibration rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Model Calibration"@en ;
    rdfs:comment "The process of adjusting model parameters to ensure model outputs align with observed data or expert judgment (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" .

ao:ParameterSelection rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Parameter Selection"@en ;
    rdfs:comment "The process of choosing appropriate values for model parameters based on data analysis, expert judgment, or other relevant information (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" .

ao:AssumptionSetting rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Assumption Setting"@en ;
    rdfs:comment "The process of establishing assumptions for use in actuarial analysis and modeling. Assumptions should be reasonable and appropriate for the intended purpose (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" .

ao:ModelDocumentation rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Model Documentation"@en ;
    rdfs:comment "The process of recording information about a model's design, development, testing, and appropriate use (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" .

### Actuarial Models and Methods

ao:ActuarialModel rdf:type owl:Class ;
    rdfs:label "Actuarial Model"@en ;
    rdfs:comment "A mathematical or statistical model used in actuarial work."@en .

ao:PricingModel rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Pricing Model"@en ;
    rdfs:comment "A model used to determine insurance premium rates."@en .

ao:ReservingModel rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Reserving Model"@en ;
    rdfs:comment "A model used to estimate claim reserves."@en .

ao:CapitalModel rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Capital Model"@en ;
    rdfs:comment "A model used to assess capital requirements and risk."@en .

ao:MortalityTable rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Mortality Table"@en ;
    rdfs:comment "A table showing death rates by age and other characteristics."@en .

ao:LossDevelopmentTriangle rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Loss Development Triangle"@en ;
    rdfs:comment "A tabular representation of loss development over time."@en .

ao:GeneralizedLinearModel rdf:type owl:Class ;
    rdfs:subClassOf ao:PricingModel ;
    rdfs:label "Generalized Linear Model (GLM)"@en ;
    rdfs:comment "A statistical model that extends linear regression for insurance pricing."@en .

ao:ChainLadderMethod rdf:type owl:Class ;
    rdfs:subClassOf ao:ReservingModel ;
    rdfs:label "Chain Ladder Method"@en ;
    rdfs:comment "A reserving method that projects ultimate claims using development factors."@en .

### ASOP 38/39: Catastrophe Modeling

ao:CatastropheModel rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Catastrophe Model"@en ;
    rdfs:comment "A model for estimating the frequency and severity of catastrophic events and their resulting losses. Catastrophe models typically combine hazard, vulnerability, and financial modules to estimate loss distributions (ASOP 38)."@en ;
    ao:asopReference "ASOP 38" .

ao:FrequencyModel rdf:type owl:Class ;
    rdfs:subClassOf ao:CatastropheModel ;
    rdfs:label "Frequency Model"@en ;
    rdfs:comment "A component of catastrophe modeling that estimates how often catastrophic events occur. Frequency models use historical data and scientific knowledge about hazard occurrence (ASOP 38)."@en ;
    ao:asopReference "ASOP 38" .

ao:SeverityModel rdf:type owl:Class ;
    rdfs:subClassOf ao:CatastropheModel ;
    rdfs:label "Severity Model"@en ;
    rdfs:comment "A component of catastrophe modeling that estimates the magnitude of losses from catastrophic events. Severity models consider vulnerability of exposed property and financial impacts (ASOP 38)."@en ;
    ao:asopReference "ASOP 38" .

ao:ScenarioAnalysis rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Scenario Analysis"@en ;
    rdfs:comment "An actuarial activity involving the analysis of specific event scenarios to estimate their potential impact. Commonly used for catastrophe or extreme event analysis to understand tail risk (ASOP 39)."@en ;
    ao:asopReference "ASOP 39" .

ao:CatastropheScenario rdf:type owl:Class ;
    rdfs:label "Catastrophe Scenario"@en ;
    rdfs:comment "A specific hypothetical or historical catastrophic event used for analysis. Examples include a magnitude 7.5 earthquake in Los Angeles or a Category 5 hurricane making landfall in Miami (ASOP 39)."@en ;
    ao:asopReference "ASOP 39" .

ao:TailRisk rdf:type owl:Class ;
    rdfs:subClassOf ao:Risk ;
    rdfs:label "Tail Risk"@en ;
    rdfs:comment "The risk of extreme losses in the tail of a probability distribution. Tail risks represent low-probability, high-severity events that can have catastrophic impact (ASOP 39)."@en ;
    ao:asopReference "ASOP 39" .

ao:ExtremeEvent rdf:type owl:Class ;
    rdfs:subClassOf ao:Event ;
    rdfs:label "Extreme Event"@en ;
    rdfs:comment "An event with unusually high severity or low frequency, typically located in the tail of the loss distribution (ASOP 39)."@en ;
    ao:asopReference "ASOP 39" .

ao:ReturnPeriod rdf:type owl:Class ;
    rdfs:subClassOf ao:Measurement ;
    rdfs:label "Return Period"@en ;
    rdfs:comment "A measurement representing the average time interval between events of a given magnitude. For example, a 100-year return period event has a 1% annual probability of occurrence (ASOP 38)."@en ;
    ao:asopReference "ASOP 38" .

### Data and Measurements

ao:Data rdf:type owl:Class ;
    rdfs:label "Data"@en ;
    rdfs:comment "Information collected and used for analysis."@en .

ao:ClaimData rdf:type owl:Class ;
    rdfs:subClassOf ao:Data ;
    rdfs:label "Claim Data"@en ;
    rdfs:comment "Data about insurance claims."@en .

ao:ExposureData rdf:type owl:Class ;
    rdfs:subClassOf ao:Data ;
    rdfs:label "Exposure Data"@en ;
    rdfs:comment "Data about units of exposure to risk."@en .

ao:PremiumData rdf:type owl:Class ;
    rdfs:subClassOf ao:Data ;
    rdfs:label "Premium Data"@en ;
    rdfs:comment "Data about insurance premiums."@en .

### ASOP 23: Data Quality

ao:DataQuality rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Data Quality"@en ;
    rdfs:comment "An assessment of the appropriateness, reliability, and limitations of data for a specific purpose. Data quality is a critical consideration in actuarial work (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" ;
    ao:ufoCategory "moment" .

ao:DataLimitation rdf:type owl:Class ;
    rdfs:label "Data Limitation"@en ;
    rdfs:comment "A constraint, deficiency, or issue affecting the usefulness or reliability of data for actuarial analysis. Actuaries should identify and disclose material data limitations (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

ao:DataDeficiency rdf:type owl:Class ;
    rdfs:subClassOf ao:DataLimitation ;
    rdfs:label "Data Deficiency"@en ;
    rdfs:comment "An inadequacy in the data that may include missing data, insufficient volume, lack of relevance, or poor quality (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

ao:DataInconsistency rdf:type owl:Class ;
    rdfs:subClassOf ao:DataLimitation ;
    rdfs:label "Data Inconsistency"@en ;
    rdfs:comment "A lack of uniformity or coherence in data, which may indicate errors or changes in data collection methods (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

ao:DataSource rdf:type owl:Class ;
    rdfs:label "Data Source"@en ;
    rdfs:comment "The origin or provider of data. Understanding the data source helps assess data quality and reliability (ASOP 23)."@en ;
    ao:asopReference "ASOP 23" .

ao:Measurement rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Measurement"@en ;
    rdfs:comment "A quantification of a property or characteristic. Measurements are moments that inhere in entities (UFO)."@en ;
    ao:ufoCategory "moment" .

ao:FinancialMeasurement rdf:type owl:Class ;
    rdfs:subClassOf ao:Measurement ;
    rdfs:label "Financial Measurement"@en ;
    rdfs:comment "A measurement of a financial quantity."@en .

ao:Estimate rdf:type owl:Class ;
    rdfs:subClassOf ao:Measurement ;
    rdfs:label "Estimate"@en ;
    rdfs:comment "An approximation or best judgment of a quantity."@en .

### ASOP 25: Credibility

ao:Credibility rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Credibility"@en ;
    rdfs:comment "A measure of the predictive value or weight given to a body of data in forming estimates. Credibility reflects the degree of confidence that a set of data can be used to predict future outcomes (ASOP 25)."@en ;
    ao:asopReference "ASOP 25" ;
    ao:ufoCategory "moment" .

ao:FullCredibility rdf:type owl:Class ;
    rdfs:subClassOf ao:Credibility ;
    rdfs:label "Full Credibility"@en ;
    rdfs:comment "A credibility level indicating that a body of data is sufficient to be given complete weight in forming an estimate, without need for supplemental data or smoothing (ASOP 25)."@en ;
    ao:asopReference "ASOP 25" .

ao:PartialCredibility rdf:type owl:Class ;
    rdfs:subClassOf ao:Credibility ;
    rdfs:label "Partial Credibility"@en ;
    rdfs:comment "A credibility level indicating that a body of data should be given partial weight, typically combined with other data sources or prior estimates to form a final estimate (ASOP 25)."@en ;
    ao:asopReference "ASOP 25" .

ao:NoCredibility rdf:type owl:Class ;
    rdfs:subClassOf ao:Credibility ;
    rdfs:label "No Credibility"@en ;
    rdfs:comment "A credibility level indicating that a body of data has insufficient predictive value to be used directly in forming estimates (ASOP 25)."@en ;
    ao:asopReference "ASOP 25" .

ao:Assumption rdf:type owl:Class ;
    rdfs:label "Assumption"@en ;
    rdfs:comment "A proposition taken to be true for the purpose of analysis or modeling."@en .

ao:ModelUncertainty rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Model Uncertainty"@en ;
    rdfs:comment "Uncertainty in model outputs arising from limitations in model structure, parameter values, or assumptions. Model uncertainty should be considered when using model results for decision-making (ASOP 56)."@en ;
    ao:asopReference "ASOP 56" ;
    ao:ufoCategory "moment" .

### Metrics and Ratios

ao:LossRatio rdf:type owl:Class ;
    rdfs:subClassOf ao:FinancialMeasurement ;
    rdfs:label "Loss Ratio"@en ;
    rdfs:comment "The ratio of losses to premiums."@en .

ao:ExpenseRatio rdf:type owl:Class ;
    rdfs:subClassOf ao:FinancialMeasurement ;
    rdfs:label "Expense Ratio"@en ;
    rdfs:comment "The ratio of expenses to premiums."@en .

ao:CombinedRatio rdf:type owl:Class ;
    rdfs:subClassOf ao:FinancialMeasurement ;
    rdfs:label "Combined Ratio"@en ;
    rdfs:comment "The sum of loss ratio and expense ratio."@en .

ao:ReturnOnEquity rdf:type owl:Class ;
    rdfs:subClassOf ao:FinancialMeasurement ;
    rdfs:label "Return on Equity (ROE)"@en ;
    rdfs:comment "A measure of profitability relative to equity."@en .

### Regulatory and Standards

ao:Regulation rdf:type owl:Class ;
    rdfs:label "Regulation"@en ;
    rdfs:comment "A rule or directive made and maintained by a regulatory authority."@en .

ao:ActuarialStandard rdf:type owl:Class ;
    rdfs:label "Actuarial Standard"@en ;
    rdfs:comment "A professional standard of practice for actuaries."@en .

ao:AccountingStandard rdf:type owl:Class ;
    rdfs:label "Accounting Standard"@en ;
    rdfs:comment "A standard for financial reporting and accounting."@en .

ao:IFRS17 rdf:type owl:Class ;
    rdfs:subClassOf ao:AccountingStandard ;
    rdfs:label "IFRS 17"@en ;
    rdfs:comment "International Financial Reporting Standard 17 for insurance contracts."@en .

ao:GAAP rdf:type owl:Class ;
    rdfs:subClassOf ao:AccountingStandard ;
    rdfs:label "GAAP"@en ;
    rdfs:comment "Generally Accepted Accounting Principles."@en .

ao:Solvency2 rdf:type owl:Class ;
    rdfs:subClassOf ao:Regulation ;
    rdfs:label "Solvency II"@en ;
    rdfs:comment "A comprehensive regulatory framework for insurance firms in the European Union. Solvency II consists of three pillars: quantitative requirements (capital requirements), supervisory review (governance and risk management), and market discipline (disclosure and transparency)."@en .

### Detailed Regulatory Framework

ao:RegulatoryReporting rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Regulatory Reporting"@en ;
    rdfs:comment "The activity of preparing and submitting required reports to regulatory authorities. Regulatory reporting ensures compliance with regulatory requirements and provides regulators with information to monitor the financial health of insurance entities."@en .

ao:RegulatoryReport rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialCommunication ;
    rdfs:label "Regulatory Report"@en ;
    rdfs:comment "A formal report submitted to regulatory authorities as required by law or regulation. Examples include annual statements, quarterly financial reports, and special regulatory filings."@en .

ao:RegulatoryRequirement rdf:type owl:Class ;
    rdfs:label "Regulatory Requirement"@en ;
    rdfs:comment "A specific requirement imposed by a regulation or regulatory authority. Regulatory requirements may relate to capital adequacy, reserving, reporting, governance, or other aspects of insurance operations."@en .

ao:CapitalRequirement rdf:type owl:Class ;
    rdfs:subClassOf ao:RegulatoryRequirement ;
    rdfs:label "Capital Requirement"@en ;
    rdfs:comment "A regulatory requirement specifying the minimum amount of capital an insurance entity must hold. Capital requirements are designed to ensure solvency and protect policyholders."@en .

ao:SolvencyCapitalRequirement rdf:type owl:Class ;
    rdfs:subClassOf ao:CapitalRequirement ;
    rdfs:label "Solvency Capital Requirement (SCR)"@en ;
    rdfs:comment "The capital required under Solvency II to ensure an insurer can meet obligations over a one-year period with 99.5% probability. The SCR can be calculated using the standard formula or an approved internal model."@en .

ao:MinimumCapitalRequirement rdf:type owl:Class ;
    rdfs:subClassOf ao:CapitalRequirement ;
    rdfs:label "Minimum Capital Requirement (MCR)"@en ;
    rdfs:comment "The minimum level of capital below which policyholders and beneficiaries would be exposed to an unacceptable level of risk. Under Solvency II, the MCR represents a lower threshold that triggers ultimate supervisory action."@en .

ao:RiskBasedCapital rdf:type owl:Class ;
    rdfs:subClassOf ao:CapitalRequirement ;
    rdfs:label "Risk-Based Capital (RBC)"@en ;
    rdfs:comment "A capital adequacy standard used in the United States that relates required capital to the risks assumed by an insurer. RBC formulas consider asset risk, underwriting risk, interest rate risk, and business risk."@en .

ao:ORSA rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialActivity ;
    rdfs:label "Own Risk and Solvency Assessment (ORSA)"@en ;
    rdfs:comment "A requirement under Solvency II and similar frameworks for insurers to assess their overall solvency needs, considering their risk profile and business strategy. The ORSA is a key component of enterprise risk management."@en .

ao:ContractualServiceMargin rdf:type owl:Class ;
    rdfs:subClassOf ao:Liability ;
    rdfs:label "Contractual Service Margin (CSM)"@en ;
    rdfs:comment "Under IFRS 17, the CSM represents the unearned profit of a group of insurance contracts that will be recognized as revenue over the coverage period. The CSM is a component of the carrying amount of insurance contract liabilities."@en .

ao:RiskAdjustment rdf:type owl:Class ;
    rdfs:subClassOf ao:Liability ;
    rdfs:label "Risk Adjustment"@en ;
    rdfs:comment "Under IFRS 17, the risk adjustment represents the compensation an entity requires for bearing the uncertainty about the amount and timing of cash flows from insurance contracts. It reflects the entity's degree of risk aversion."@en .

ao:RegulatoryApproval rdf:type owl:Class ;
    rdfs:label "Regulatory Approval"@en ;
    rdfs:comment "A formal authorization or permission granted by a regulatory authority. Regulatory approvals may be required for new products, rate changes, mergers and acquisitions, or changes to internal models."@en .

ao:InternalModel rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialModel ;
    rdfs:label "Internal Model"@en ;
    rdfs:comment "A risk measurement system developed by an insurer to calculate capital requirements based on its own risk profile. Under Solvency II, internal models require regulatory approval and must meet standards for use, statistical quality, calibration, profit and loss attribution, validation, and documentation."@en .

### ASOP 41: Actuarial Communications

ao:ActuarialCommunication rdf:type owl:Class ;
    rdfs:subClassOf ao:Object ;
    rdfs:label "Actuarial Communication"@en ;
    rdfs:comment "A written, electronic, or oral communication issued by an actuary with respect to actuarial services. Includes reports, opinions, presentations, and other work products (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

ao:ActuarialReport rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialCommunication ;
    rdfs:label "Actuarial Report"@en ;
    rdfs:comment "A formal written actuarial communication documenting actuarial services, findings, and recommendations (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

ao:ActuarialOpinion rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialCommunication ;
    rdfs:label "Actuarial Opinion"@en ;
    rdfs:comment "A formal statement by an actuary expressing professional judgment regarding the adequacy of reserves, adequacy of capital, or other actuarial matters (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

ao:StatementOfActuarialOpinion rdf:type owl:Class ;
    rdfs:subClassOf ao:ActuarialOpinion ;
    rdfs:label "Statement of Actuarial Opinion"@en ;
    rdfs:comment "A formal regulatory opinion on the adequacy of reserves or financial condition, often required by law. Typically provided by an appointed actuary."@en ;
    ao:asopReference "ASOP 36, ASOP 41" .

ao:Disclosure rdf:type owl:Class ;
    rdfs:label "Disclosure"@en ;
    rdfs:comment "Information that an actuary must disclose in an actuarial communication per professional standards. Includes data limitations, assumptions, methods, reliance on others, etc. (ASOP 41)."@en ;
    ao:asopReference "ASOP 41" .

ao:IntendedUser rdf:type owl:Class ;
    rdfs:subClassOf ao:Agent ;
    rdfs:label "Intended User"@en ;
    rdfs:comment "A role played by an agent identified by the actuary as able to rely on the actuarial communication. Communications should be appropriate for the intended user (ASOP 41)."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" ;
    ao:asopReference "ASOP 41" .

ao:AppointedActuary rdf:type owl:Class ;
    rdfs:subClassOf ao:Actuary ;
    rdfs:label "Appointed Actuary"@en ;
    rdfs:comment "A role played by an actuary who is appointed by an organization or designated by a regulator to provide specific actuarial opinions, typically regarding reserve adequacy."@en ;
    ao:rigidity "anti-rigid" ;
    ao:ufoCategory "role" .

### UFO-C: Social and Intentional Entities

ao:Commitment rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Commitment"@en ;
    rdfs:comment "A social moment that binds an agent to a course of action. Commitments create obligations and expectations (UFO-C)."@en ;
    ao:ufoCategory "moment" .

ao:PolicyCommitment rdf:type owl:Class ;
    rdfs:subClassOf ao:Commitment ;
    rdfs:label "Policy Commitment"@en ;
    rdfs:comment "An insurer's commitment to pay benefits under specified conditions. The binding obligation created by an insurance policy (UFO-C)."@en .

ao:Right rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Right"@en ;
    rdfs:comment "A claim or entitlement held by an agent. Rights are social moments that derive from commitments (UFO-C). Not to be confused with insurance claim."@en ;
    ao:ufoCategory "moment" .

ao:Goal rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Goal"@en ;
    rdfs:comment "A desired state of affairs or outcome that an agent intends to achieve. Goals are intentional moments (UFO-C)."@en ;
    ao:ufoCategory "moment" .

ao:RiskManagementGoal rdf:type owl:Class ;
    rdfs:subClassOf ao:Goal ;
    rdfs:label "Risk Management Goal"@en ;
    rdfs:comment "A goal related to identifying, assessing, mitigating, or transferring risks."@en .

ao:Intention rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Intention"@en ;
    rdfs:comment "A mental moment directed at achieving a goal. Intentions represent an agent's commitment to pursue a particular course of action (UFO-C)."@en ;
    ao:ufoCategory "moment" .

ao:Belief rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Belief"@en ;
    rdfs:comment "A mental moment representing an agent's acceptance of a proposition as true. Beliefs inform decision-making and risk assessment (UFO-C)."@en ;
    ao:ufoCategory "moment" .

ao:Obligation rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Obligation"@en ;
    rdfs:comment "A social moment that binds an agent to perform a specific action or refrain from it. Obligations arise from commitments and social relationships (UFO-C)."@en ;
    ao:ufoCategory "moment" .

ao:PolicyObligation rdf:type owl:Class ;
    rdfs:subClassOf ao:Obligation ;
    rdfs:label "Policy Obligation"@en ;
    rdfs:comment "An obligation arising from an insurance policy, such as the policyholder's obligation to pay premiums or the insurer's obligation to pay claims."@en .

### Professional Practice Concepts

ao:ProfessionalJudgment rdf:type owl:Class ;
    rdfs:subClassOf ao:Moment ;
    rdfs:label "Professional Judgment"@en ;
    rdfs:comment "An actuary's reasoned opinion or conclusion based on their professional knowledge, training, and experience. Professional judgment involves weighing available evidence, considering alternatives, and making informed decisions when faced with uncertainty or incomplete information."@en ;
    ao:ufoCategory "moment" .

ao:Deviation rdf:type owl:Class ;
    rdfs:label "Deviation"@en ;
    rdfs:comment "A departure from guidance in an Actuarial Standard of Practice. When an actuary deviates from guidance in an applicable ASOP, the actuary should disclose the deviation and provide the reason for it in accordance with ASOP 41."@en .

ao:PeerReview rdf:type owl:Class ;
    rdfs:subClassOf ao:Activity ;
    rdfs:label "Peer Review"@en ;
    rdfs:comment "A review of actuarial work by another qualified actuary to assess its quality, appropriateness, and compliance with professional standards. Peer review helps ensure work quality and provides independent verification of actuarial conclusions."@en .

ao:QualityControl rdf:type owl:Class ;
    rdfs:subClassOf ao:Activity ;
    rdfs:label "Quality Control"@en ;
    rdfs:comment "Systematic procedures to ensure actuarial work meets quality standards. Quality control may include checking calculations, reviewing assumptions, validating models, and documenting work."@en .

ao:AuditTrail rdf:type owl:Class ;
    rdfs:label "Audit Trail"@en ;
    rdfs:comment "A documented record of the steps, decisions, and rationale in actuarial work. An audit trail enables others to understand and reproduce the work, facilitating review, verification, and regulatory compliance."@en .

ao:WorkPaper rdf:type owl:Class ;
    rdfs:label "Work Paper"@en ;
    rdfs:comment "Documentation supporting actuarial work, including data sources, methods, assumptions, calculations, and results. Work papers provide the detail behind actuarial communications and enable review of the work."@en .

### End of Ontology
