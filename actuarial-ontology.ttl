    rdfs:label "generates scenario"@en ;
    rdfs:comment "Relates a scenario generation activity to scenarios it produces."@en .

ao:containsPolicy rdf:type owl:ObjectProperty ;
    rdfs:domain ao:Portfolio ;
    rdfs:range ao:InsurancePolicy ;
    rdfs:label "contains policy"@en ;
    rdfs:comment "Relates a portfolio to the individual policies it contains."@en .

ao:belongsToPortfolio rdf:type owl:ObjectProperty ;
    rdfs:domain ao:InsurancePolicy ;
    rdfs:range ao:Portfolio ;
    rdfs:label "belongs to portfolio"@en ;
    rdfs:comment "Relates an insurance policy to the portfolio it belongs to."@en ;
