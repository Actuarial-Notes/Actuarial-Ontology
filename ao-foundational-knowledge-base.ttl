    rdfs:label "Earthquake" ;
    rdfs:comment "Seismic ground shaking causing structural damage and ground failure" .

kb:Peril_Flood rdf:type ao:NaturalPeril ;
    rdfs:label "Flood" ;
    rdfs:comment "Overflow of water onto normally dry land from rivers, storm surge, or heavy rainfall" .

kb:Peril_Wildfire rdf:type ao:NaturalPeril ;
    rdfs:label "Wildfire" ;
