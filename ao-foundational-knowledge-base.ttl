#################################################################
#    Risks
#################################################################

### Mortality and Longevity Risks

kb:Risk_Mortality rdf:type ao:MortalityRisk , ao:InsurableRisk ;
    rdfs:label "Mortality Risk" ;
    rdfs:comment "Risk of death occurring earlier than expected, affecting life insurance and annuity death benefits" .

kb:Risk_Longevity rdf:type ao:LongevityRisk , ao:InsurableRisk ;
    rdfs:label "Longevity Risk" ;
    rdfs:comment "Risk of individuals living longer than expected, affecting annuity payouts and pension obligations" .

kb:Risk_Morbidity rdf:type ao:MorbidityRisk , ao:InsurableRisk ;
    rdfs:label "Morbidity Risk" ;
    rdfs:comment "Risk of illness, injury, or disability affecting health insurance and disability income products" .

### Property and Casualty Risks

kb:Risk_Property rdf:type ao:PropertyRisk , ao:InsurableRisk ;
    rdfs:label "Property Damage Risk" ;
    rdfs:comment "Risk of physical damage to insured property from covered perils" .

kb:Risk_Liability rdf:type ao:LiabilityRisk , ao:InsurableRisk ;
    rdfs:label "Liability Risk" ;
    rdfs:comment "Risk of legal liability for bodily injury or property damage to third parties" .

kb:Risk_Catastrophe rdf:type ao:CatastropheRisk , ao:InsurableRisk ;
    rdfs:label "Catastrophe Risk" ;
    rdfs:comment "Risk of extreme events causing widespread insured losses across large geographic areas" .

### Financial Risks

kb:Risk_Market rdf:type ao:MarketRisk ;
    rdfs:label "Market Risk" ;
    rdfs:comment "Risk of adverse changes in market values of investments affecting assets and variable products" .

kb:Risk_Interest_Rate rdf:type ao:InterestRateRisk , ao:MarketRisk ;
    rdfs:label "Interest Rate Risk" ;
    rdfs:comment "Risk of changes in interest rates affecting asset values, liability values, and product profitability" .

kb:Risk_Credit rdf:type ao:CreditRisk ;
    rdfs:label "Credit Risk" ;
    rdfs:comment "Risk of counterparty default on bonds, reinsurance recoverables, and other financial obligations" .

kb:Risk_Operational rdf:type ao:OperationalRisk ;
    rdfs:label "Operational Risk" ;
    rdfs:comment "Risk of loss from inadequate or failed internal processes, people, systems, or external events" .

kb:Risk_Liquidity rdf:type ao:LiquidityRisk ;
    rdfs:label "Liquidity Risk" ;
    rdfs:comment "Risk of inability to meet cash flow obligations when due without incurring unacceptable losses" .

### Emerging Risks

kb:Risk_Cyber rdf:type ao:CyberRisk , ao:EmergingRisk ;
    rdfs:label "Cyber Risk" ;
    rdfs:comment "Risk of data breaches, cyber attacks, ransomware, and technology system failures" .

kb:Risk_Pandemic rdf:type ao:PandemicRisk , ao:EmergingRisk ;
    rdfs:label "Pandemic Risk" ;
    rdfs:comment "Risk of widespread disease outbreak affecting mortality, morbidity, and business operations" .

kb:Risk_Climate rdf:type ao:ClimateRisk , ao:EmergingRisk ;
    rdfs:label "Climate Change Risk" ;
    rdfs:comment "Risk of increased frequency and severity of weather-related events due to climate change" .

