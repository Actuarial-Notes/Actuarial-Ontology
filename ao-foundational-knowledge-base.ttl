kb:Peril_Water_Damage rdf:type ao:ManmadePeril ;
    rdfs:label "Water Damage" ;
    rdfs:comment "Water damage from burst pipes, appliance failures, or roof leaks" .

kb:Peril_Theft rdf:type ao:ManmadePeril ;
    rdfs:label "Theft / Burglary" ;
    rdfs:comment "Unauthorized taking of property" .

kb:Peril_Vandalism rdf:type ao:ManmadePeril ;
    rdfs:label "Vandalism" ;
    rdfs:comment "Willful or malicious damage to property" .

kb:Peril_Terrorism rdf:type ao:ManmadePeril ;
    rdfs:label "Terrorism" ;
    rdfs:comment "Violent acts intended to create fear or intimidation" .

